from = <%= @email_from %>
to = <%= @email_to %>
smtppassword = <%= @smtp_pass %>
smtpuser = <%= @smtp_user %>
smtphost = <%= @smtp_host %>
smtpport = 25
subject = ClamWin Virus Alert

[Updates]
checkversion = 0
checkversionurl = http://clamwin.sourceforge.net/clamwinver.php
warnoutofdate = 0
enable = 0
dbupdatelogfile = C:\ProgramData\.clamwin\log\ClamUpdateLog.txt
dbmirror = database.clamav.net
updateonlogon = 0
frequency = Daily
weekday = 2
time = 19:35:00

