[EmailScan]
scanoutgoing = 0
scanincoming = 0
showsplash = 0

[Schedule]
path =

[ClamAV]
infectedonly = 1
maxfilesize = 100
showprogress = 1
excludepatterns = *.dbx|CLAMWIN_SEP|*.tbb|CLAMWIN_SEP|*.pst|CLAMWIN_SEP|*.dat|CLAMWIN_SEP|*.log|CLAMWIN_SEP|*.evt|CLAMWIN_SEP|*.nsf|CLAMWIN_SEP|*.ntf|CLAMWIN_SEP|*.chm
kill = 1
priority = Low
logfile = C:\ProgramData\.clamwin\log\ClamScanLog.txt
freshclam = C:\Program Files (x86)\ClamWin\bin\freshclam.exe
scanarchives = 1
clamscan = C:\Program Files (x86)\ClamWin\bin\clamscan.exe
maxscansize = 150
includepatterns =
maxfiles = 500
scanole2 = 1
quarantinedir = C:\ProgramData\.clamwin\quarantine
maxlogsize = 1
moveinfected = 0
freshclamparams =
enablembox = 0
scanrecursive = 1
database = C:\ProgramData\.clamwin\db
maxrecursion = 50
clamscanparams =
debug = 0
removeinfected = 1

[UI]
reportinfected = 0
traynotify = 0
version = 0.98.5
standalone = 0

[Proxy]
host =
password =
port = 3128
user =

[EmailAlerts]
